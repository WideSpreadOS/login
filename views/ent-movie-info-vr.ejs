

<a-scene>
    <a-assets>
    </a-assets>


    <a-entity geometry="primitive: plane" position="0 1 -4" scale="16 9 0.5" material="shader: html; target: #htmlElement"></a-entity>
    
    <div style="width: 100%; height: 100%; position: fixed; top: 0; left: 0; z-index: -1; overflow: hidden;">
        <iframe style="position: absolute;  left: 0; top: 0; width: 100%; height: 100%;" id="htmlElement" src="https://www.2embed.ru/embed/imdb/movie?id=<%= returnedData.imdbID %>" frameborder="0"></iframe>
    </div>
    <a-entity camera look-controls position="0 1.6 0"></a-entity>
    <a-sky color="#111"></a-sky>
</a-scene>
