}

input.toggle:checked ~ #show-menu .btn:nth-child(3) {
  top:-40px;left:50px;opacity:1;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
}

input.toggle:checked ~ #show-menu .btn:nth-child(4) {
  left:70px;opacity:1;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
}

input.toggle:checked ~ #show-menu .btn:nth-child(5) {
  top :50px;left:50px;opacity:1;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
}

input.toggle:checked ~ #show-menu .btn:nth-child(6) {
  top:70px;opacity:1;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
}

input.toggle:checked ~ #show-menu .btn:nth-child(7) {
  top:50px;left:-50px;opacity:1;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
}

input.toggle:checked ~ #show-menu .btn:nth-child(8) {
  left:-70px;opacity:1;-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
