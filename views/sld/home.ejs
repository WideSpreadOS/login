<div class="sld-container">
    <div class="page-title">
        <h5>SLD</h5>
    </div>

    <div class="search">
        <form action="/sld/video/search" method="post">
            <input type="search" name="query" placeholder="Search for videos...">
            <button type="submit">Search</button>
        </form>
    </div>



    <div class="sld-video-container">
        <% returnedData.videos.forEach(function(data) { %>
            <a href="/sld/video/id/<%= data.id %>">
            <div class="sld-video">
                <div class="thumbnail">
                    <img src="<%= data.default_thumb.src %>" alt="">
                </div>
                <div class="spread-menu">
                    <div class="menu">
    <input type="checkbox" id="<%= data.id %>" class="toggle"/>
    <label id="show-menu" for="<%= data.id %>">
      <div class="btn">
        <i class="fas fa-project-diagram toggleBtn menuBtn"></i>
        <i class="material-icons md-36 toggleBtn closeBtn">close</i>
      </div>
      <div class="btn" title="Make your background image - Dashboard & Public Profile">
          <form action="/users/change-main-background-image-url?_method=PATCH" method="post">
            <input type="text" name="bgImage" value="<%= data.default_thumb.src %>" hidden>
            <button type="submit"><i class="far fa-image"></i></button>
        </form>
      </div>
      <div class="btn" title="Like this">
        <i class="far fa-heart"></i>
      </div>
      <div class="btn" title="Share this">
        <i class="fas fa-share-alt"></i>
      </div>
      <div class="btn" title="Comment on this">
        <i class="far fa-comment-alt"></i>
      </div>
      <div class="btn" title="Save this">
        <i class="far fa-save"></i>
      </div>
      <div class="btn" title="Add this">
        <form action="/users/save-video-url?_method=PATCH" method="post">
          <input type="text" name="videoId" value="<%= data.id %>" hidden>
          <input type="text" name="poster" value="<%= data.default_thumb.src %>" hidden>
          <button type="submit"><i class="fas fa-plus"></i></button>
      </form>
      </div>
      <div class="btn" title="Find more near this location">
        <i class="fas fa-search-location"></i>
      </div>
      <div class="btn" title="More Information">
        <i class="fas fa-info"></i>
      </div>
    </label>
  </div>
                </div>
                <div class="sld-video-background">
                    <iframe src="<%= data.embed %>" frameborder="0"></iframe>
                </div>
                <div class="sld-video-header">
                    <div class="video-data">
                        <div class="video-data-title">
                            <p><%= data.title %></p>
                            <p style="color: pink; font-size: 0.8rem; text-align: right;"><%= data.length_min %></p>
                            <div class="tags-slider">
                                <h6><%= data.keywords %></h6>
                            </div>
                        </div>
                    </div>


                </div>

                <div class="thumbnail-slide">
                    <% data.thumbs.forEach(function(thumb) { %>
                        <img src="<%= thumb.src %>" alt="">
                    <% }) %>
                </div>
            </div>
        </a>
        <% }) %>
    </div>
</div>