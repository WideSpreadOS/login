<h1><%= courseData.course %></h1>


<% courseData.classes.forEach(function(classes){ %>
    <div class="class-container">
        <h1><%= classes.name %></h1>
        <p><%= classes.description %></p>
        <div class="nav-bar">
            <a href="/academy/courses/<%= courseData.id %>/<%= classes.id %>">Begin Class</a>
        </div>
    </div>
    <% }); %>
